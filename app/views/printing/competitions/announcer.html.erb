<h2><%= @competition %> Competition Order</h2>

<table class="full_grid">
  <thead>
    <tr>
      <th>Order</th>
      <th>ID</th>
      <th>Name</th>
      <th>Club</th>
    </tr>
  </thead>
  <tbody>
    <% @competition.competitors.each do |comp| %>
      <tr>
        <td><%= comp.position %></td>
        <td><%= comp.external_id %></td>
        <td><%= comp %></td>
        <td><%= comp.club %></td>
      </tr>
    <% end %>
  </tbody>
</table>
