%nav.top-bar#main_menu.hide-for-small-only{ data: { topbar: true }, role: "navigation" }
  - if EventConfiguration.configuration_exists?
    .top-bar-title
      = render "nav_bar/logo"
  %div
    .top-bar-left
      %ul.menu.horizontal
        = render "nav_bar/my_links"
    - if user_signed_in?
      .top-bar-right
        %ul.menu.horizontal
          %li= link_to t('.sign_out'), destroy_user_session_path, :method => :delete
