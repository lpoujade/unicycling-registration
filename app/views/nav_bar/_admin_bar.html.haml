-#- cache_i18n ['navbar', current_user, @config, Category, Competition] do
- if user_signed_in? && current_user.roles.any?
  %nav.top-bar.hide-for-small-only{ data: { topbar: true }, role: "navigation" }
    %section.top-bar-section
      %ul.left
        = render "nav_bar/admin_set_one"
      %ul.right
        %li.has-dropdown
          %a{ href: "#" } Admin Bar 2
          %ul.dropdown
            = render "nav_bar/admin_set_two"
