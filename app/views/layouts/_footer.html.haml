.non_printable#bottom_nav
  .choose_language
    %ul
      - I18n.available_locales.each do |locale|
        %li= link_to t("language_name", locale: locale), new_locale_path(locale), hreflang: locale, lang: locale
  = render "/attending/rulebook_link"

  = link_to t('.help'), help_welcome_path
  - if Page.find_by(slug: 'privacy-policy')
    = link_to t('.privacy_policy'), page_path('privacy-policy')
    = link_to "Unicycling Event Organizers", example_description_path
  %p= t(".developed_by")
