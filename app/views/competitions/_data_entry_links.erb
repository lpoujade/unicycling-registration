<tr>
  <td><%= competition %></td>
  <td>
    <%= link_to "Announcer Form", announcer_printing_competition_path(competition) %>
    <%= link_to "(pdf)", announcer_printing_competition_path(competition, :format => :pdf) %>
  </td>

  <td>
    <%= data_recording_link(competition, is_start_times: true) %>
  </td>
  <td>
    <%= start_data_entry_link(current_user, competition) %>
  </td>
  <td>
    <%= data_confirmation_link(competition, is_start_times: true) %>
  </td>

  <td>
    <%= data_recording_link(competition, is_start_times: false) %>
  </td>
  <td>
    <%= end_data_entry_link(current_user, competition) %>
  </td>
  <td>
    <%= data_confirmation_link(competition, is_start_times: false) %>
  </td>
</tr>
