%h1 Data Entry for #{@competition}


= link_to "Proof", proof_single_user_competition_import_results_path(@user, @competition)
= link_to "(pdf)", proof_single_user_competition_import_results_path(@user, @competition, format: :pdf)

%h2= "Competition: #{@competition}"
%h3= "Start Times: #{@is_start_time}"

%h1 Listing Imported Results

= form_tag(destroy_all_user_competition_import_results_path(@user, @competition), {:method => :delete, :multipart=>true}) do
  = submit_tag "Delete all in Temp Table", :class => "data_import_delete", data: {:confirm => "DELETE, Are you sure?" }
= form_tag(approve_user_competition_import_results_path(@user, @competition), {:method => :post}) do
  = submit_tag "Import all in Temp Table into Competition", :class=> "data_import_submit", data: {:confirm => "IMPORT, Are you sure?" }
%hr

#notice
- @import_result ||= ImportResult.new
= render partial: "form"
