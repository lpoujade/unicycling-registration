%table.import_results
  %thead
    %tr
      - if display_username
        %th Username
      %th Entry Number
      %th Bib number
      - if @competition.imports_times
        %th Time
      - else
        %th Rank (External)
      %th Disqualified
      %th Name
      %th Competitor Exists?
  %tbody
    - @import_results.each_with_index do |result, index|
      %tr
        - if display_username
          %td= result.user
        %td= index + 1
        %td= result.bib_number
        - if @competition.imports_times
          %td= result.full_time
        - else
          %td= result.rank
        %td= result.disqualified
        %td= Registrant.find_by_bib_number(result.bib_number)
        - if result.competition.find_competitor_with_bib_number(result.bib_number)
          %td.competitor_exists Yes
        - else
          %td.competitor_not_exists No

%hr
