- column_count_per_attempt = 1 # minutes
- column_count_per_attempt += 1 # seconds
- column_count_per_attempt += 1 if @competition.data_entry_format.hours?
- column_count_per_attempt += 1 if @competition.data_entry_format.thousands?
- column_count_per_attempt += 1 if @competition.data_entry_format.hundreds?

%thead
  %tr
    %th{rowspan: "2"}
    %th{rowspan: "2"} Bib Number
    %th{rowspan: "2"} Competitor Name
    %th{rowspan: "2"} Competitor Exists?
    %th{rowspan: "2"}
    %th{colspan: column_count_per_attempt + 2} Attempt #1
    %th{colspan: column_count_per_attempt + 2} Attempt #2
    %th{colspan: "2"}
  %tr
    = render "attempt_header"
    = render "attempt_header"
    %th
    %th
