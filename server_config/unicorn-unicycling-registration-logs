/home/ec2-user/unicycling-registration/current/log/*log {
    create 0644 ec2-user ec2-user
    daily
    rotate 30
    missingok
    notifempty
    compress
    sharedscripts
    postrotate
        source /home/ec2-user/.rvm/environments/ruby-2.2.3@unicycling-registration
        cd /home/ec2-user/unicycling-registration/current/ && bundle exec eye user_command rotate all
    endscript
}
